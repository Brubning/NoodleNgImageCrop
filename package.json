{
  "name": "noodle-ng-image-crop",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rimraf dist && ng build",
    "package": "rimraf dist && ng-packagr -p package.json",
    "publish-npm": "yarn package && yarn pack && yarn publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@angular/core",
      "@angular/common"
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^10.2.5",
    "@angular/core": "10.2.5",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "rxjs": "^6.6.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler": "^10.2.5",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@types/jasmine": "^2.8.17",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7"
  },
  "description": "Angular image cropping component. It is a port of directive-based code from https://github.com/bcabanes/angular-image-cropper to an Angular 10 component.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brubning/NoodleNgImageCrop.git"
  },
  "keywords": [
    "Angular",
    "ImageCrop"
  ],
  "author": "David Brunning",
  "license": "MIT",
  "main": "dist/bundles/noodle-ng-image-crop.umd.js",
  "files": [
    "src",
    "dist",
    "angular.json",
    "package.json",
    "package-lock.json",
    "README.md",
    "tsconfig.json",
    "tslint.json"
  ],
  "bugs": {
    "url": "https://github.com/Brubning/NoodleNgImageCrop/issues"
  },
  "homepage": "https://github.com/Brubning/NoodleNgImageCrop#readme"
}
