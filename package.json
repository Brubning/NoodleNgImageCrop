{
  "name": "noodle-ng-image-crop",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rimraf dist && ng build",
    "package": "rimraf dist && ng-packagr -p package.json",
    "publish-npm": "yarn package && yarn pack && yarn publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "allowedNonPeerDependencies": [
      "@angular/core",
      "@angular/common",
      "zone.js"
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^14.1.2",
    "@angular/core": "^14.1.2",
    "zone.js": "^0.11.8"
  },
  "peerDependencies": {
    "rxjs": "^6.6.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular/cli": "^14.1.2",
    "@angular/compiler": "^14.1.2",
    "@angular/compiler-cli": "^14.1.2",
    "@angular/language-service": "^14.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.55",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^14.1.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "description": "Angular image cropping component. It is a port of directive-based code from https://github.com/bcabanes/angular-image-cropper to an Angular 11 component.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brubning/NoodleNgImageCrop.git"
  },
  "keywords": [
    "Angular",
    "ImageCrop"
  ],
  "author": "David Brunning",
  "license": "MIT",
  "main": "dist/bundles/noodle-ng-image-crop.umd.js",
  "files": [
    "src",
    "dist",
    "angular.json",
    "package.json",
    "package-lock.json",
    "README.md",
    "tsconfig.json",
    "tslint.json"
  ],
  "bugs": {
    "url": "https://github.com/Brubning/NoodleNgImageCrop/issues"
  },
  "homepage": "https://github.com/Brubning/NoodleNgImageCrop#readme"
}
